{
  "schemaVersion": 1,
  "id": "exnihilofabrico",
  "name": "Ex Nihilo Fabrico",
  "description": "Fabricate from nothing.",
  "authors": ["SirLyle"],
  "version": "${version}",
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "mixins": [
    "exnihilofabrico.common.json"
  ],
  "entrypoints": {
    "main": [{
        "adapter": "kotlin",
        "value": "exnihilofabrico.ExNihiloFabrico"}],
    "client": [{
      "adapter": "kotlin",
      "value": "exnihilofabrico.ExNihiloFabricoClient"}],
    "rei_plugins_v0": ["exnihilofabrico.compatibility.rei.PluginEntry"]
  },
  "depends": {
    "fabric": "*",
    "fabricloader": ">=0.4.6",
    "fabric-language-kotlin": ">=1.3.40",
    "towelette": "*",
    "artifice": "*",
    "libblockattributes_items": "*",
    "libblockattributes_fluids": "*"
  }
}
